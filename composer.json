{
  "name": "jarzon/primpack",
  "description": "Helper Pack base for Prim projects",
  "type": "project",
  "autoload": {
    "psr-4": {"PrimPack\\": "/"}
  },
  "autoload-dev": {
    "psr-4": {"Tests\\": "tests/"}
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "require": {
    "php": ">=8.4.0",
    "jarzon/prim": "^1.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1.0",
    "phpstan/extension-installer": "^1.4.0",
    "phpstan/phpstan-deprecation-rules": "^2.0.0"
  }
}
